{
  "name": "nuxt-app",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "check": "npm run typecheck && npm run lint:debug && npm run build",
    "clean": "nuxt cleanup && rimraf .output/ ./dist",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare && npm run sort",
    "lint": "eslint .",
    "lint:debug": "cross-env DEBUG=eslint:cli-engine eslint . --fix",
    "lint:fix": "eslint . --fix",
    "preview": "nuxt preview",
    "sort": "npx sort-package-json",
    "typecheck": "nuxt typecheck"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@nuxt/devtools": "latest",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "nuxt": "^3.8.1",
    "rimraf": "^5.0.5",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  }
}
